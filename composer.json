    }, 
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
    },
    "suggest": {
    },
    "conflict": {
    },
    "archive": {
        "exclude": [
            "test",
            "hack",
            "hooks",
            ".arclint"
        ]
    },
    "config": {
        "bin-dir":             "bin",
        "store-auths":         false,
        "optimize-autoloader": true,
        "sort-packages":       true,
        "apcu-autoloader":     true,
        "notify-on-install":   false
    },
    "minimum-stability": "stable"
